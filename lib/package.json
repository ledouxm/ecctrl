    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.6.0"
  },
  "dependencies": {
    "@react-three/drei": "^9.83.3",
    "@react-three/fiber": "^8.13.3",
    "@react-three/rapier": "^1.1.1",
